{
  "name": "@matters/matters-editor",
  "version": "0.2.0-alpha.1",
  "description": "Editor for matters.news",
  "author": "https://github.com/thematters",
  "homepage": "https://github.com/thematters/matters-editor",
  "repository": {
    "type": "git",
    "url": "git@github.com:thematters/matters-editor.git"
  },
  "engines": {
    "node": ">=16.14 <17.0"
  },
  "license": "MIT",
  "type": "module",
  "main": "./dist/main.umd.cjs",
  "module": "./dist/main.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:examples": "node --loader ts-node/esm --experimental-specifier-resolution=node ./bin/build-examples.ts",
    "preview": "vite preview",
    "format": "prettier --write \"{src,}/**/*.{ts,tsx,json}\"",
    "lint": "tsc && eslint",
    "test": "vitest --run",
    "prepare": "husky install",
    "benchmark": "node --loader ts-node/esm --experimental-specifier-resolution=node ./benchmark/index.ts"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/natural-compare-lite": "^1.4.0",
    "@types/node": "^18.14.2",
    "@types/react": "^17.0.53",
    "@types/react-dom": "^17.0.19",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@vitejs/plugin-react": "^3.1.0",
    "benny": "^3.7.1",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "husky": "^8.0.3",
    "natural-compare-lite": "^1.4.0",
    "prettier": "^2.8.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "vite": "^4.1.0",
    "vite-plugin-dts": "^2.0.2",
    "vitest": "^0.29.1"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "hast-util-to-html": "^8.0.4",
    "rehype-format": "^4.0.1",
    "rehype-parse": "^8.0.4",
    "rehype-raw": "^6.1.1",
    "rehype-remark": "^9.1.2",
    "rehype-rewrite": "^3.0.6",
    "rehype-sanitize": "^5.0.1",
    "rehype-stringify": "^9.0.3",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remark-stringify": "^10.0.2",
    "unified": "^10.1.2"
  }
}
